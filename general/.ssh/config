Host gitlab.com github.com
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_main

Host aur.archlinux.org
    IdentityFile ~/.ssh/id_aur
    User ~/.ssh/id_aur

Host *
  ForwardAgent no
  Compression no
  ServerAliveInterval 0
  ServerAliveCountMax 3
  HashKnownHosts no
  UserKnownHostsFile ~/.ssh/known_hosts
  ControlMaster no
  ControlPath ~/.ssh/master-%r@%n:%p
  ControlPersist no
