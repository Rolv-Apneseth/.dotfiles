<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<unique-id>1623153185934582-1</unique-id>
	<command>&quot;$TERMINAL&quot; %d</command>
	<description>Open the default terminal at current directory</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>media-playback-start</icon>
	<name>Open With Media Player</name>
	<unique-id>1650284790159409-1</unique-id>
	<command>&quot;$MY_MEDIA_PLAYER&quot; %F</command>
	<description>Open the file with the default media player</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<video-files/>
</action>
<action>
	<icon>nvim</icon>
	<name>Open With Neovim</name>
	<unique-id>1650284976307665-2</unique-id>
	<command>&quot;$TERMINAL&quot; -e nvim %F</command>
	<description>Open folder(s)/file(s) with neovim</description>
	<patterns>*</patterns>
	<directories/>
	<other-files/>
	<text-files/>
</action>
</actions>
